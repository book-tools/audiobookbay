{
  "name": "@book-tools/audiobookbay",
  "version": "2.0.6",
  "description": "AudioBookBay Javascript API",
  "license": "ISC",
  "author": "Chris Sandvik",
  "repository": {
    "type": "git",
    "url": "https://github.com/book-tools/audiobookbay"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "coverage": "vitest run --coverage",
    "example": "tsx ./playground/usage-example.ts",
    "lint": "npm run lint:src && npm run lint:types",
    "lint:src": "eslint .",
    "lint:types": "tsc",
    "test": "vitest"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "filesize-parser": "^1.5.1",
    "html-to-text": "^9.0.5",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/cheerio": "^0.22.35",
    "@types/filesize-parser": "^1.5.3",
    "@types/html-to-text": "^9.0.4",
    "@types/node-fetch": "^2.6.12",
    "@vitest/coverage-v8": "^3.0.6",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.5.1",
    "tsup": "^8.3.6",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.6"
  },
  "keywords": [
    "audiobook",
    "audiobookbay",
    "javascript",
    "torrent"
  ]
}
