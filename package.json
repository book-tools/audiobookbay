{
  "name": "audiobookbay",
  "version": "2.0.6",
  "description": "AudioBookBay Scraper JavaScript",
  "license": "ISC",
  "author": "Valentin Lica",
  "repository": {
    "type": "git",
    "url": "https://github.com/licavalentin/audiobookbay"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup",
    "coverage": "vitest run --coverage",
    "example": "esno ./playground/usage-example.ts",
    "test": "vitest"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node-fetch": "^2.6.12",
    "esno": "^4.8.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.5.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "keywords": [
    "audiobook",
    "audiobookbay",
    "javascript",
    "torrent"
  ]
}
